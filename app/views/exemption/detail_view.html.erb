        type: 'pie',
        height: '250px',
        sliceColors: ['#00FFFF','#3366cc','#dc3912','#FF1493','#66aa00','#FF7F50','#0099c6','#990099','#DB7093','#DEB887','#8B4513'],
        //sliceColors: ['#1ab394','#B5B8CF'],
