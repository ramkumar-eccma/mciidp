<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
         <div class="col-lg-6">
            <div class="ibox float-e-margins" style="border:1px solid #23C6C8;">
                <div class="ibox-title text-primary" style="background:#23C6C8;">
                    <h5 style="color:#f3f3f4 !important;">Classes</h5>
                    <div class="ibox-tools">
                        <a class="collapse-link" style="color:#f3f3f4 !important;">
                            <i class="fa fa-chevron-up" ></i>
                        </a>               
                        <a class="close-link hide" style="color:#f3f3f4 !important;">
                            <i class="fa fa-times"></i>
                        </a>
                    </div>
                </div>
                <div class="ibox-content text-center h-200">
                    <span id="ksa_class" class="ksa_class"></span>
                    <div class="row" style="padding-top:10px;">
                         <div class="col-md-12">
                            <div class="pull-left" style="width:15px;height:15px;;background-color:#ffff;"></div>
                            <div class="pull-left"> &nbsp;<a href="<%=  url_for :controller => 'madein_ksa', :action => 'showclass',:TYPE => 'all'%>" target="_blank">&nbsp;&nbsp;Total(<%= @classname_count.inspect  %>)</a></div><br>
                            <div class="pull-left" style="width:15px;height:15px;;background-color:#66AA01;"></div>
                            <div class="pull-left"> &nbsp;<a href="<%= url_for :controller => 'madein_ksa', :action => 'showclass',:TYPE => 'OPERATIONAL' %>" target="_blank">&nbsp;&nbsp;CABLE,POWER,ELECTRICAL (<%= @ksa_cable.inspect  %>)</a></div><br>
                             <div class="pull-left" style="width:15px;height:15px;;background-color:#DC3912;"></div>
                            <div class="pull-left"> &nbsp;<a href="<%=  url_for :controller => 'madein_ksa', :action => 'showclass',:TYPE => 'Cancel Suggestion'%>" target="_blank">&nbsp;&nbsp;PIPE,METALLIC (<%= @ksa_bar.inspect %>)</a></div><br>
                              <div class="pull-left" style="width:15px;height:15px;;background-color:#3366CC;"></div>
                            <div class="pull-left"> &nbsp;<a href="<%=  url_for :controller => 'madein_ksa', :action => 'showclass',:TYPE => 'CANCELLED'%>" target="_blank">&nbsp;&nbsp;WIRE,ELECTRICAL (<%= @ksa_wire.inspect %>)</a></div><br>
                        </div>  
                         <div class="col-md-12">
                             <div class="pull-left" style="width:15px;height:15px;;background-color:#49C5E8;"></div>
                            <div class="pull-left"> &nbsp;<a href="<%= url_for :controller => 'madein_ksa', :action => 'showclass',:TYPE => 'UNDER CONSTRUCTION' %>" target="_blank">&nbsp;&nbsp;BATTERY,STORAGE (<%= @ksa_battery.inspect %>)</a></div><br>
                        </div>
                            <div class="col-lg-6 pull-left">
                                <div class="pull-left" style="width:15px;height:15px;;background-color:#FF7F50;"></div>
                                 <div class="pull-left"> &nbsp;<a href="<%= url_for :controller => 'madein_ksa', :action => 'showclass',:TYPE => 'UNDER CONSTRUCTION' %>" target="_blank">&nbsp;&nbsp;Others (<%= @ksa_others.inspect %>)</a></div>
                            </div>
                            <div class="col-md-6">
                                <a href="<%= url_for :controller => 'madein_ksa',:action => 'more_details' %>" target="_blank" class="btn btn-xs btn-outline btn-success pull-right">More ..... <i class="fa fa-long-arrow-right"></i> </a>
                        </div>                                
                    </div>              
                </div>
            </div>
        </div>
    </div>
</div>
<% content_for :javascript do %>

