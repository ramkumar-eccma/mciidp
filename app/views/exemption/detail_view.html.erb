     $(".dbref_cmm").sparkline(['<%= @dbref_inv.inspect %>','<%= @dbref_locinv.inspect %>','<%= @dbref_match.inspect %>','<%= @dbref_nonoff.inspect %>','<%= @dbref_matchgmv.inspect %>','<%= @dbref_matchfrv.inspect %>','<%= @dbref_matchukv.inspect %>','<%= @dbref_nonoffgmv.inspect %>','<%= @dbref_nonoffrnv.inspect %>','<%= @dbref_nonoffuk.inspect %>','<%= @dbref_other.inspect %>'], {
