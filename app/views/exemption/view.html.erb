<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox" style="border:1px solid #49C5E8;">
                <div class="ibox-title" style="background:#49C5E8;color:#f3f3f4 !important;">
                     <%- if I18n.locale== :ar %>
            تفاصيل مادية
                <% else %>
                   Material Details
                <% end -%>
              <div class="ibox-tools hide">
                  <a class="collapse-link" style="color:#f3f3f4 !important;">
                      <i class="fa fa-chevron-up"></i>
                  </a>
                 
                  <a class="close-link" style="color:#f3f3f4 !important;">
                      <i class="fa fa-times"></i>
                  </a>
              </div>
                </div>
                    <div class="ibox-content">
                        <table class="table table-bordered">
                            <% @data.each do |a|%>
                            <tr>
                                <th>CMM MATERIAL NO</th>
                                <th>MATERIAL ID</th>
                                <th>DATE CREATED</th>
                            </tr>
                            <tr>
                                <td><%=a.cmm_material_no%></td>
                                <td><%=a.material_id%></td>
                                <td><%=a.datecreated%></td>
                            </tr>
                             <% end %>
                        </table>
                    </div>
            </div>
        </div>
        <div class="col-lg-12">
            <div class="ibox" style="border:1px solid #23C6C8;">
                    <div class="ibox-title" style="background:#23C6C8;color:#f3f3f4 !important;">
                          <%- if I18n.locale==:ar%>
                               <span class="">
                                <% @ar_breadcrum_desc = Language.select("language_property").where("language_id = ?  AND language_type = ? ", '43','ar')%>
                                <% @ar_breadcrum_desc.each do |ar_breadcrum_desc|%>
                               <%= ar_breadcrum_desc.language_property %>
                                <% end %>
                            <% else %>
                                  <% @ar_breadcrum_desc = Language.select("language_property").where("language_id= ? AND language_type = ?",'43','en')%>
                                  <% @ar_breadcrum_desc.each do |en_breadcrum_desc|%>
                                  <%= en_breadcrum_desc.language_property %>
                            <% end %>
                          <% end -%>
                    </div>
                        <div class="ibox-content">
                         <table class="table table-bordered">
                            <%@data.each do |b|%>
                            <tr>
                                <th>SHORT DESCRIPTION</th>
                                <td><%=b.short_description%></td>
                            </tr>
                            <tr>
                                <th>LONG DESCRIPTION</th>
                                <td><%=b.long_description%></td>
                            </tr>
                            <% end %>
                        </table>
                       </div>
            </div>
        </div>
