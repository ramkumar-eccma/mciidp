<div class="wrapper wrapper-content animated fadeInRight">
<div class="row">
    <div class="col-lg-12">
        <div class="ibox" style="border:1px solid #0C78B8;">
            <div class="ibox-title" style="background:#0C78B8;">
                <h5 style="color:#f3f3f4 !important;"><%- if I18n.locale== :ar %>
                    <span class="">
                      <% @ar_classif = Language.select("language_property").where("language_id = ?  AND language_type = ? ", '38','ar')%>
                      <% @ar_classif.each do |ar_classif|%>
                      <%= ar_classif.language_property %>
                       <% end %>
                    </span>
                  <% else %>
                    <span class="">
                      <% @en_classif = Language.select("language_property").where("language_id = ?  AND language_type = ? ", '38','en')%>
                      <% @en_classif.each do |en_classif|%>
                      <%= en_classif.language_property %>
                       <% end %>
                    </span>
                <% end -%></h5>
                <div class="ibox-tools hide">
                    <a class="collapse-link">
                        <i class="fa fa-times" style="color:#f3f3f4 !important;"></i>
                    </a>
                </div>
            </div>
            <div class="ibox-content">
                <% @cage_details.each do |t| %>
                   <table class="table table-bordered" width="100%" style="table-layout: fixed; word-wrap: break-word;"> 
                        <tr> 
                            <td width="40%"><strong>CAGE_CD </strong></td>
                            <td width="60%"><%= t.CAGE_CD %> </td>
                        </tr>
                        <tr> 
                            <td><strong>RP_CAGE </strong></td>
                            <td><%= t.RP_CAGE %> </td>
                        </tr>
                        <tr> 
                            <td><strong>CAGE_Name</strong></td>
                            <td><%= t.CAGE_Name %></td>
                        </tr>
                        <tr> 
                            <td><strong>ST_ADRS_1 </strong></td>
                            <td><%= t.ST_ADRS_1 %> </td>
                        </tr>
                        <tr> 
                            <td><strong> ST_ADRS_2 </strong></td>
                            <td><%= t.ST_ADRS_2 %> </td>
                        </tr>
                        <tr> 
                            <td><strong>POBOX </strong></td>
                            <td><%= t.POBOX %> </td>
                        </tr>
                        <tr> 
                            <td><strong>CITY </strong></td>
                            <td><%= t.CITY %> </td>
                        </tr>
                        <tr> 
                            <td><strong>ZIP_CD </strong></td>
                            <td><%= t.ZIP_CD %> </td>
                        </tr>
                        <tr> 
                            <td><strong>COUNTRY </strong></td>
                            <td><%= t.COUNTRY %> </td>
                        </tr>
                        <tr> 
                            <td><strong>TEL_NBR </strong></td>
                            <td><%= t.TEL_NBR %> </td>
                        </tr>
                    </table>
                <% end %>
            </div>
        </div>
    </div>
</div>
