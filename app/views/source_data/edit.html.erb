<% content_for :javascript do %>
  <script>
    $(function() {    


     
      $('#technical td:nth-child(4)').hide(); 
      $('#technical th:nth-child(4)').hide(); 
      $('#technical td:nth-child(5)').hide(); 
      $('#technical th:nth-child(5)').hide(); 
      $('#technical td:nth-child(6)').hide(); 
      $('#technical th:nth-child(6)').hide(); 
      $('#toggle').click(function() {
        $('#technical td:nth-child(4)').toggle(); 
        $('#technical th:nth-child(4)').toggle(); 
      $('#technical td:nth-child(5)').toggle(); 
      $('#technical th:nth-child(5)').toggle(); 
      $('#technical td:nth-child(6)').toggle(); 
      $('#technical th:nth-child(6)').toggle(); 
        
      });
    });

    function cage(cval)
    {
     // openModal();
    if (window.XMLHttpRequest)
      {// code for IE7+, Firefox, Chrome, Opera, Safari
      xmlhttp=new XMLHttpRequest();
      }
    else
      {// code for IE6, IE5
      xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
      }
    xmlhttp.onreadystatechange=function()
      {
      if (xmlhttp.readyState==4 && xmlhttp.status==200)
        {
         // json_func();
          //closeModal();
        document.getElementById("result").innerHTML=xmlhttp.responseText;
        }
      }
     // qid=document.getElementById("cat_id1").value
    xmlhttp.open("GET","vendor?vendor="+cval,true);
    xmlhttp.send();

    }
