    //         //document.getElementById('modal').style.display = 'block';
    //         document.getElementById('abc').style.display = 'block';
    // }

    // function closeModal() {
    //     //document.getElementById('modal').style.display = 'none';
    //     document.getElementById('abc').style.display = 'none';
    // }

    </script>
<% end %>
    
